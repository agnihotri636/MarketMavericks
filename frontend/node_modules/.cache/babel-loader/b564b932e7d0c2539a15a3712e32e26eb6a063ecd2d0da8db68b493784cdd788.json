{"ast":null,"code":"export default {\n  name: 'Home',\n  data: function data() {\n    return {\n      companies: ['Apple', 'Google', 'Amazon', 'Microsoft'],\n      // Example list of companies\n      selectedCompany: '',\n      quantity: 0,\n      buyPrice: 0,\n      sellPrice: 0,\n      profitLoss: null\n    };\n  },\n  methods: {\n    addTrade: function addTrade() {\n      var totalCost = this.quantity * this.buyPrice;\n      var totalRevenue = this.quantity * this.sellPrice;\n      this.profitLoss = totalRevenue - totalCost; // Calculate profit/loss\n    }\n  }\n};","map":{"version":3,"names":["name","data","companies","selectedCompany","quantity","buyPrice","sellPrice","profitLoss","methods","addTrade","totalCost","totalRevenue"],"sources":["/home/shivam/Desktop/tradetrek/frontend/src/views/Home.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <h1>Welcome to TradeTrek</h1>\n    <p>Your personal trading journal and analysis tool</p>\n    <h2>Features:</h2>\n    <ul>\n      <li>Track your trades</li>\n      <li>Analyze your performance</li>\n      <li>Set and monitor trading goals</li>\n      <li>Secure and private space</li>\n    </ul>\n\n    <h2>Track Trades</h2>\n    <form @submit.prevent=\"addTrade\">\n      <div class=\"form-group\">\n        <label for=\"company\">Select Company:</label>\n        <select v-model=\"selectedCompany\" id=\"company\">\n          <option v-for=\"company in companies\" :key=\"company\" :value=\"company\">{{ company }}</option>\n        </select>\n      </div>\n\n      <div class=\"form-group\">\n        <label for=\"quantity\">Quantity:</label>\n        <input type=\"number\" v-model.number=\"quantity\" id=\"quantity\" required />\n      </div>\n\n      <div class=\"form-group\">\n        <label for=\"buyPrice\">Buy Price:</label>\n        <input type=\"number\" v-model.number=\"buyPrice\" id=\"buyPrice\" required />\n      </div>\n\n      <div class=\"form-group\">\n        <label for=\"sellPrice\">Sell Price:</label>\n        <input type=\"number\" v-model.number=\"sellPrice\" id=\"sellPrice\" required />\n      </div>\n\n      <button type=\"submit\">Add Trade</button>\n    </form>\n\n    <h3 v-if=\"profitLoss !== null\">Profit/Loss: {{ profitLoss }}</h3>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'Home',\n  data() {\n    return {\n      companies: ['Apple', 'Google', 'Amazon', 'Microsoft'], // Example list of companies\n      selectedCompany: '',\n      quantity: 0,\n      buyPrice: 0,\n      sellPrice: 0,\n      profitLoss: null,\n    };\n  },\n  methods: {\n    addTrade() {\n      const totalCost = this.quantity * this.buyPrice;\n      const totalRevenue = this.quantity * this.sellPrice;\n      this.profitLoss = totalRevenue - totalCost; // Calculate profit/loss\n    },\n  },\n};\n</script>\n\n<style scoped>\n.home {\n  text-align: center;\n  padding: 20px;\n}\n\n.form-group {\n  margin: 10px 0;\n}\n\nlabel {\n  display: block;\n  margin-bottom: 5px;\n}\n\ninput,\nselect {\n  padding: 8px;\n  width: 200px;\n  margin-bottom: 10px;\n}\n</style>\n"],"mappings":"AA4CA,eAAe;EACbA,IAAI,EAAE,MAAM;EACZC,IAAI,WAAJA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,CAAC,OAAO,EAAE,QAAQ,EAAE,QAAQ,EAAE,WAAW,CAAC;MAAE;MACvDC,eAAe,EAAE,EAAE;MACnBC,QAAQ,EAAE,CAAC;MACXC,QAAQ,EAAE,CAAC;MACXC,SAAS,EAAE,CAAC;MACZC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,QAAQ,WAARA,QAAQA,CAAA,EAAG;MACT,IAAMC,SAAQ,GAAI,IAAI,CAACN,QAAO,GAAI,IAAI,CAACC,QAAQ;MAC/C,IAAMM,YAAW,GAAI,IAAI,CAACP,QAAO,GAAI,IAAI,CAACE,SAAS;MACnD,IAAI,CAACC,UAAS,GAAII,YAAW,GAAID,SAAS,EAAE;IAC9C;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}